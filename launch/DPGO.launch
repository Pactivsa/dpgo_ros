  <param name="/dataset" type="str" value="$(find dpgo_ros)/data/$(arg dataset)/$(arg dataset).g2o" />
  <param name="/Xopt" type="str" value="$(find dpgo_ros)/data/$(arg dataset)/Xopt.txt" />
  <param name="/Xinit" type="str" value="$(find dpgo_ros)/data/$(arg dataset)/Xinit.txt" /> -->

  <!-- Launch visualization in rviz -->
